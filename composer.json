{
    "name": "louis-l/once",
    "description": "A magic memoization function",
    "keywords": [
        "once",
        "callable",
        "memoization",
        "cache"
    ],
    "homepage": "https://github.com/louis-l/once",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "benmorel/weakmap-polyfill": "^0.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Once\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Once\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
  }
}
